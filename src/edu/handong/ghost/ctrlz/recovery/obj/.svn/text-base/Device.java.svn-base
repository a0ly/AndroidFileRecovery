public class Device {

	private String devPath;
	private String mountDir;
	private String fsType;
	private String mountOpts;
	private String recoveryDir;
	private long totalSize;
	private long freeSize;
	private int pathCount;
	
	public Device() { }
	
	public void setDevPath(String devPath){
		this.devPath = devPath;
	}
	public void setMountDir(String mountDir){
		this.mountDir = mountDir;
	}
	public void setFsType(String fsType){
		this.fsType = fsType;
	}
	public void setMountOpts(String mountOpts){
		this.mountOpts = mountOpts;
	}
	public void setRecoveryDir(String recoveryDir){
		this.recoveryDir = recoveryDir;
	}
	public void setTotalSize(long totalSize){
		this.totalSize = totalSize;
	}
	public void setFreeSize(long freeSize){
		this.freeSize = freeSize;
	}
	public void setPathCount(int pathCount){
		this.pathCount = pathCount;
	}
	public String getDevPath(){
		return devPath;
	}
	public String getMountDir(){
		return mountDir;
	}
	public String getFsType(){
		return fsType;
	}
	public String getMountOpts(){
		return mountOpts;
	}
	public String getRecoveryDir(){
		return recoveryDir;
	}
	public long getTotalSize(){
		return totalSize;
	}
	public long getFreeSize(){
		return freeSize;
	}
	public int getPathCount(){
		return pathCount;
	}
}
