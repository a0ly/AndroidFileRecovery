public class RecoverFile {
	
	private String fileName;
	private String extender;
	private String url;
	private String dirPath;
	private long size;		// byte unit
	private int expectedPossibility;
	private boolean isThumnailGen;
	
	public RecoverFile() { 
		isThumnailGen=false;
	}

	public void setFileName(String fileName){
		this.fileName = fileName;
	}
	public void setExtender(String extender){
		this.extender = extender;
	}
	public void setUrl(String url){
		this.url = url;
	}
	public void setDirPath(String dirPath){
		this.dirPath = dirPath;
	}
	public void setSize(long size){
		this.size = size;
	}
	public void setExpectedPossibility(int expectedPossiblity){
		this.expectedPossibility = expectedPossiblity;
	}
	public void setThumnailGenFlag(){
		isThumnailGen = true;
	}
	public String getFileName(){
		return fileName;
	}
	public String getExtender(){
		return extender;
	}
	public String getUrl(){
		return url;
	}
	public String getDirPath(){
		return dirPath;
	}
	public long getSize(){
		return size;
	}
	public int getExpectedPossibility(){
		return expectedPossibility;
	}
	public boolean isThumnailGen(){
		return isThumnailGen;
	}

}
